# Example run macro for qsim

# Set run mode
/qsim/fSourceMode 1

/run/initialize 
/process/optical/boundary/setInvokeSD true

/qsim/filename rootfiles/qsim_out_smFull-v3_slac5gev_1.root
/qsim/seed 50 

/qsim/zmin -25 mm
/qsim/zmax -25 mm

#/qsim/ymin -15 mm
#/qsim/ymax 15 mm

#/qsim/xmin 0.0 mm
#/qsim/xmax 0.0 mm

/qsim/emin 5.0 GeV
